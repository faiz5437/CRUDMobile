<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gambar1"
    tools:context=".ReadBiodata">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/vNim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/nama"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="48dp"
        android:layout_marginTop="68dp"
        android:fontFamily="@font/poppins_bold"
        android:text="NIM"
        android:textColor="#0971F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/NIM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="116dp"
        android:fontFamily="@font/poppins"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/vNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/nama"
        android:layout_alignLeft="@+id/vNim"
        android:layout_alignBottom="@+id/vNim"
        android:layout_marginStart="48dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Nama"
        android:textColor="#0971F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vNim" />

    <TextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/NIM"
        android:layout_alignRight="@+id/NIM"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="116dp"
        android:fontFamily="@font/poppins"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NIM" />

    <TextView
        android:id="@+id/vtgl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tgl"
        android:layout_alignBottom="@+id/tgl"
        android:layout_marginStart="48dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Tanggal Lahir"
        android:textColor="#0971F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vNama" />

    <TextView
        android:id="@+id/tgl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nama"
        android:layout_alignLeft="@+id/nama"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="116dp"
        android:fontFamily="@font/poppins"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nama" />

    <TextView
        android:id="@+id/vJenisKelamin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/jenisKelamin"
        android:layout_alignBottom="@+id/jenisKelamin"
        android:layout_marginStart="48dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Jenis Kelamin"
        android:textColor="#0971F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vtgl" />

    <TextView
        android:id="@+id/jenisKelamin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tgl"
        android:layout_alignLeft="@+id/tgl"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="116dp"
        android:fontFamily="@font/poppins"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tgl" />

    <TextView
        android:id="@+id/alamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jenisKelamin"
        android:layout_alignLeft="@+id/jenisKelamin"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="116dp"
        android:fontFamily="@font/poppins"
        android:text="TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jenisKelamin" />

    <TextView
        android:id="@+id/vAlamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/alamat"
        android:layout_alignLeft="@+id/vJenisKelamin"
        android:layout_alignBottom="@+id/alamat"
        android:layout_marginStart="48dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Alamat"
        android:textColor="#0971F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vJenisKelamin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button1"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/vAlamat"
        android:layout_alignLeft="@+id/vAlamat"
        android:layout_marginStart="52dp"
        android:layout_marginTop="80dp"
        android:drawableLeft="@drawable/ic_arrow"
        android:text="Kembali"
        android:textColor="#0971F1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vAlamat" />


</androidx.constraintlayout.widget.ConstraintLayout>